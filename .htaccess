<IfModule mod_rewrite.c>
    RewriteEngine On
    # RewriteBase /  <-- REMOVE THIS LINE

    # If the request is not for a file or directory that exists, rewrite to index.html
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /index.html [L]
</IfModule>
